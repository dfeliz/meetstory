{
    "manifest_version": 2,
    "content_security_policy": "default-src 'none'; script-src 'self' https://apis.google.com; connect-src 'self'; img-src * 'self' data:; style-src 'self' 'unsafe-inline';base-uri 'self';form-action 'self'",
    "name": "Meetstory",
    "description": "Tool for saving google meets chat history",
    "version": "0.1.0",
    "browser_action": {
        "default_title": "Meetstory",
        "default_popup": "pages/popup.html"
    },
    "content_scripts": [
        {
          "matches": ["https://meet.google.com/*"],
          "js": ["pages/content_scripts.js"]
        }
    ],
    "background": {
        "page": "pages/background.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "identity"
    ],
    "options_page": "pages/options.html",
    "icons": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "32": "icons/icon32.png",
        "128": "icons/icon128.png"
    },
    "key": ""
}

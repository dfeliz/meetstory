{
    "name": "Meetstory",
    "description": "Tool for saving google meets chat history",
    "version": "0.1.0",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Meetstory"
    },
    "manifest_version": 2   ,
    "permissions": [
        "storage"
        ],
    "icons": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "32": "icons/icon32.png",
        "128": "icons/icon128.png"
    }
}